<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="db_log_cleaning" instance="LotsofPixels\Logcleaner\Cron\Logcleaner" method="execute">
            <!-- Every monday at 6:05 server time -->
            <schedule>5 6 * * 1 </schedule>
        </job>
    </group>
</config>